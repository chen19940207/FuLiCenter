<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cn.ucai.fulicenter">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name=".application.FuLiCenterApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".controller.Activity.MainActivity" />
        <activity android:name=".controller.Activity.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".controller.Activity.BoutiqueActivity" />
        <activity android:name=".controller.Activity.GoodsDetailsActivity" />
        <activity android:name=".controller.Activity.CategoryChildActivity" />
        <activity android:name=".controller.Activity.LoginActivity" />
        <activity android:name=".controller.Activity.RegisterActivity" />
        <activity android:name=".controller.Activity.SettingsActivity" />
        <activity android:name=".controller.Activity.UpdateNickActivity"></activity>
    </application>

</manifest>